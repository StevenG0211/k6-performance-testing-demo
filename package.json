{
    "name": "k6-demo",
    "version": "1.0.0",
    "description": "k6 performance testing project with TypeScript",
    "scripts": {
        "test:api": "k6 run src/tests/api/basic-api.test.ts",
        "test:browser": "k6 run src/tests/browser/basic-browser.test.ts",
        "test:load": "k6 run src/scenarios/load.test.ts",
        "test:stress": "k6 run src/scenarios/stress.test.ts",
        "test:spike": "k6 run src/scenarios/spike.test.ts",
        "test:soak": "k6 run src/scenarios/soak.test.ts",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\""
    },
    "keywords": [
        "k6",
        "performance-testing",
        "load-testing",
        "typescript"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/k6": "^1.3.1",
        "@typescript-eslint/eslint-plugin": "^8.46.2",
        "@typescript-eslint/parser": "^8.46.2",
        "eslint": "^9.39.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "prettier": "^3.6.2",
        "typescript": "^5.9.3"
    }
}
